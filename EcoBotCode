        description="–≠—Ç–æ—Ç –±–æ—Ç –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ —ç–∫–æ–ª–æ–≥–∏—á–µ—Å–∫–∏—Ö –ø—Ä–æ–±–ª–µ–º–∞—Ö –∏ –ø—Ä–µ–¥–ª–∞–≥–∞–µ—Ç –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã–µ —Ñ–∞–∫—Ç—ã.",
        color=0x1abc9c  # –¶–≤–µ—Ç –≤—Å—Ç—Ä–∞–∏–≤–∞–µ–º–æ–≥–æ —Å–æ–æ–±—â–µ–Ω–∏—è
    )
    embed.add_field(
        name="–ö–æ–º–∞–Ω–¥—ã:",
        value=(
            "**!–Ω–æ–≤–æ—Å—Ç–∏ [–∫–∞—Ç–µ–≥–æ—Ä–∏—è]** - –ü–æ–ª—É—á–∏—Ç—å –ø–æ—Å–ª–µ–¥–Ω–∏–µ –Ω–æ–≤–æ—Å—Ç–∏ –ø–æ –≤—ã–±—Ä–∞–Ω–Ω–æ–π –∫–∞—Ç–µ–≥–æ—Ä–∏–∏ (–ø–æ —É–º–æ–ª—á–∞–Ω–∏—é '—ç–∫–æ–ª–æ–≥–∏—è').\n"
            "**!—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏** - –ü–æ–ª—É—á–∏—Ç—å —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ –ø–æ —Å–Ω–∏–∂–µ–Ω–∏—é —ç–∫–æ–ª–æ–≥–∏—á–µ—Å–∫–æ–≥–æ –≤–ª–∏—è–Ω–∏—è.\n"
            "**!–∫–∞—Ä—Ç–∞** - –ü–æ–∫–∞–∑–∞—Ç—å –∫–∞—Ä—Ç—É —ç–∫–æ–ª–æ–≥–∏—á–µ—Å–∫–∏—Ö –∏–Ω–∏—Ü–∏–∞—Ç–∏–≤.\n"
            "**!—Ñ–∞–∫—Ç—ã** - –ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã–µ —Ñ–∞–∫—Ç—ã –æ –∑–∞–≥—Ä—è–∑–Ω–µ–Ω–∏–∏ –æ–∫—Ä—É–∂–∞—é—â–µ–π —Å—Ä–µ–¥—ã.\n"
            "**!quizz** - –ü—Ä–æ–π—Ç–∏ –∫–≤–∏–∑ –Ω–∞ –æ—Å–Ω–æ–≤–µ —Ñ–∞–∫—Ç–æ–≤ –∏ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π."
        ),
        inline=False
    )
    await ctx.send(embed=embed)

@bot.command(name='–Ω–æ–≤–æ—Å—Ç–∏')
async def get_news(ctx, category='—ç–∫–æ–ª–æ–≥–∏—è'):
    params = {
        'q': category,
        'apiKey': NEWS_API_KEY,
        'pageSize': 5
    }
    response = requests.get(NEWS_API_URL, params=params)
    news_data = response.json()
    
    if news_data['totalResults'] == 0:
        await ctx.send(f"–ù–æ–≤–æ—Å—Ç–∏ –ø–æ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏ '{category}' –Ω–µ –Ω–∞–π–¥–µ–Ω—ã.")
    else:
        for article in news_data['articles']:
            await ctx.send(f"üì∞ **{article['title']}**\n{article['description']}\n–ò—Å—Ç–æ—á–Ω–∏–∫: {article['url']}")

@bot.command(name='—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏')
async def get_recommendations(ctx):
    recommendations_sample = random.sample(recommendations, 5)
    await ctx.send("üåø –í–æ—Ç –Ω–µ—Å–∫–æ–ª—å–∫–æ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π –ø–æ —Å–Ω–∏–∂–µ–Ω–∏—é —ç–∫–æ–ª–æ–≥–∏—á–µ—Å–∫–æ–≥–æ –≤–ª–∏—è–Ω–∏—è:")
    for recommendation in recommendations_sample:
        await ctx.send(f"‚Ä¢ {recommendation}")

@bot.command(name='—Ñ–∞–∫—Ç—ã')
async def get_facts(ctx):
    facts_sample = random.sample(facts, 5)
    await ctx.send("üìä –ò–Ω—Ç–µ—Ä–µ—Å–Ω—ã–µ —Ñ–∞–∫—Ç—ã –æ –∑–∞–≥—Ä—è–∑–Ω–µ–Ω–∏–∏ –æ–∫—Ä—É–∂–∞—é—â–µ–π —Å—Ä–µ–¥—ã:")
    for fact in facts_sample:
        await ctx.send(fact)

@bot.command(name='quizz')
async def start_quizz(ctx):
    await ctx.send(
        "üß© –ö–≤–∏–∑ –Ω–∞ –æ—Å–Ω–æ–≤–µ —Ñ–∞–∫—Ç–æ–≤ –∏ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π! "
        "–ü–µ—Ä–µ–¥ –Ω–∞—á–∞–ª–æ–º –∏–≥—Ä—ã –ª—É—á—à–µ –æ–∑–Ω–∞–∫–æ–º—å—Ç–µ—Å—å —Å –∫–æ–º–∞–Ω–¥–∞–º–∏ !—Ñ–∞–∫—Ç—ã –∏ !—Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏.\n"
        "–ù–∞—á–∏–Ω–∞–µ–º –∏–≥—Ä—É! –í—Å—Ç–∞–≤—å—Ç–µ –ø—Ä–æ–ø—É—â–µ–Ω–Ω—ã–µ —Å–ª–æ–≤–∞ –≤ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–∏."
    )

    score = 0
    questions = random.sample(quiz_questions, 5)
    
    for question, answer in questions:
        await ctx.send(f"‚ùì {question}")
        try:
            def check(m):
                return m.author == ctx.author and m.channel == ctx.channel
            
            response = await bot.wait_for('message', check=check, timeout=30.0)
            
            if response.content.lower() == answer.lower():
                await ctx.send("‚úÖ –ü—Ä–∞–≤–∏–ª—å–Ω–æ!")
                score += 1
            else:
                await ctx.send(f"‚ùå –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ. –ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç: {answer}")
        except:
            await ctx.send(f"‚è∞ –í—Ä–µ–º—è –≤—ã—à–ª–æ! –ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç: {answer}")
    
    await ctx.send(f"üéâ –í—ã –Ω–∞–±—Ä–∞–ª–∏ {score} –∏–∑ 5 –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤!")

bot.run("–ó–¥–µ—Å—å –Ω–∏—á–µ–≥–æ –Ω–µ—Ç")
